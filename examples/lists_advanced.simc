FUNC make_list(a, b, c) -> [a, b, c]
make_list(1, 2, 3)

FUNC get_first(list) -> list / 0
VAR mylist = [10, 20, 30]
get_first(mylist)

FUNC append_item(list, item) -> list + item
append_item([1, 2, 3], 4)

VAR result = IF 5 > 3 THEN [1, 2, 3] ELSE [4, 5, 6]
result

VAR nums = [1, 2, 3, 4, 5]
VAR sum = 0
FOR i = 0 TO 5 THEN VAR sum = sum + (nums / i)
sum
